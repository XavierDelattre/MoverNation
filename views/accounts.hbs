<div class="page-header header-filter" data-parallax="true" style="background-image: url('/assets/img/bg4.jpg')">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h1 class="title">Movers are all accross the world.</h1>
        <h4>Because be a Mover is a to be part of a community, you can see what the community share with you.</h4>
      </div>
    </div>
  </div>
</div>
<div class="main main-raised">
	<div class="container">
		<div class="section text-center">
			<div class="row">
          <div class="col-md-8 ml-auto mr-auto">
            <h2 class="title">List of Movers</h2>
            <h5 class="description">MoverNation bla bla bla bla</h5>
            <br><br>
            <div class="row">
            	<div class="col-md-8 ml-auto mr-auto">
		            <table class="table">
		            	<thead>
		            		<tr>
		            			<th>Username</th>
		            			<th class="text-center">ID</th>
		            		</tr>
		            	</thead>
		            	<tbody>
		            		{{#each listUsers}}
		            		<tr>
		            			<th><a href="/accounts/{{this.id}}">{{this.username}}</a></th>
		            			<th class="text-center">{{this.id}}</th>
		            		</tr>
		            		{{/each}}
		            	</tbody>
		            </table>
		          </div>
          	</div>
          </div>
      </div>
		</div>
	</div>
</div>